{
    "$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion":2,
	"title":"BetterDocs Archive Doc List",
	"name":"betterdocs/archive-doc-list",
	"category":"betterdocs",
	"description":"Streamlines the creation of your knowledge-base archive lists.",
	"textdomain":"betterdocs",
    "editorScript":"betterdocs-blocks-editor",
    "attributes": {
        "layout": {
            "type": "string",
            "default": "layout-1"
        },
        "listEntryHeadingTag": {
            "type": "string",
            "default": "h3"
        },
        "nested_subcategory": {
            "type": "boolean",
            "default": false
        },
        "orderby": {
            "type": "string",
            "default": "title"
        },
        "order": {
            "type": "string",
            "default": "asc"
        },
        "postsPerPageLayoutTwo": {
            "type": "number",
            "default": -1
        },
        "pagination": {
            "type": "boolean",
            "default": false
        }
    }
}
